<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.springapp.mvc">

    <class name="Employee" table="employee">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="account" column="account" />
        <property name="graduate" column="isGraduate"/>
        <property name="name" column="name"/>
        <property name="onceOut" column="onceOut"/>
        <property name="rate" column="rate"/>
        <property name="timeInTW" column="timeInTW"/>
        <property name="timeOnThisAccount" column="account_time"/>
        <property name="timeToJoin" column="join_time"/>
        <property name="totalWorkYear" column="totalWorkYear"/>
    </class>

</hibernate-mapping>
